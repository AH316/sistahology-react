{
  "name": "sistahology-react",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:regression": "dotenv -e .env.test -- playwright test --project=authUser --grep @regression",
    "test:journals": "dotenv -e .env.test -- playwright test --project=authUser --grep @journals",
    "test:security": "dotenv -e .env.test -- playwright test tests/security.spec.ts",
    "snap:home": "playwright test homepage-hero.spec.ts --project=chromium --update-snapshots",
    "snap:journal": "dotenv -e .env.test -- playwright test --project=authUser tests/journals-first-create.spec.ts --grep 'should handle journal creation across different viewports'",
    "test:install": "playwright install",
    "set:admin": "tsx scripts/setAdminRole.ts --email testadmin@example.com"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@supabase/supabase-js": "^2.53.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^24.1.0",
    "@types/sanitize-html": "^2.16.0",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "focus-trap-react": "^11.0.4",
    "lucide-react": "^0.534.0",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.61.1",
    "react-router-dom": "^7.7.1",
    "sanitize-html": "^2.17.0",
    "tailwindcss": "^4.1.11",
    "zod": "^4.0.17",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/js": "^9.30.1",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/dompurify": "^3.0.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "dotenv": "^17.2.1",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "playwright": "^1.55.0",
    "tsx": "^4.20.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  }
}
